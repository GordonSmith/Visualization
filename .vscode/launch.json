{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8000/demos/dermatology.html?src/marshaller/HTML.wu&designMode=false",
            "sourceMaps": true,
            "webRoot": "http://localhost:8000/demos",
            "pathMapping": {
                "/": "${workspaceRoot}/"
            }
        },
        {
            "name": "ECL Watch",
            "type": "chrome",
            "request": "launch",
            "url": "http://192.168.3.22:8010",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Launch Chrome against localhost, with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8000/demos/dermatology.html?src/marshaller/HTML.databomb",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}