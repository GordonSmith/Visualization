{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "file:///C:/Users/gordon/git/hpcc-js/packages/viz/demos/dermatology.html",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/demos",
            "pathMapping": {
                "/": "${workspaceRoot}/"
            }
        },
        {
            "name": "Launch Chrome against localhost, with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}