{
  "private": true,
  "name": "@hpcc-js/demo-wecare",
  "version": "0.0.52",
  "scripts": {
    "clean": "rimraf lib* && rimraf dist",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "webpack",
    "bundle-watch": "npm run bundle -- -w",
    "bundle2": "rollup -c",
    "build": "npm run compile-es6 && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-es6-watch\" \"npm run bundle-watch\""
  },
  "dependencies": {
    "@hpcc-js/chart": "^0.0.72",
    "@hpcc-js/common": "^0.0.70",
    "@hpcc-js/comms": "^0.0.75",
    "@hpcc-js/dgrid": "^0.0.81",
    "@hpcc-js/form": "^0.0.72",
    "@hpcc-js/map": "^0.0.75",
    "d3-selection": "1.1.0",
    "d3-time-format": "2.1.0",
    "impress.js": "GordonSmith/impressjs.git#MILEHIGH_UI"
  },
  "devDependencies": {
    "@types/d3": "4.9.0",
    "concurrently": "3.5.0",
    "rimraf": "2.6.2",
    "rollup": "0.54.0",
    "source-map-loader": "0.2.1",
    "typescript": "2.6.1",
    "webpack": "3.10.0"
  }
}