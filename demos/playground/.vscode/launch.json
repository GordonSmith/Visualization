{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dashy-dev",
            "type": "chrome",
            "request": "launch",
            "url": "file:///${workspaceRoot}/index.html",
            "runtimeArgs": [
                "--allow-file-access-from-files",
                "--disable-web-security"
            ],
            "webRoot": "${workspaceRoot}"
        }
    ]
}