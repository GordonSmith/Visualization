{
    "private": true,
    "name": "@hpcc-js/demo-storyboard",
    "main": "dist/index.js",
    "module": "dist/index.es6",
    "unpkg": "dist/index.min.js",
    "jsdelivr": "dist/index.min.js",
    "types": "types/index.d.ts",
    "typesVersions": {
        "<3.8": {
            "*": [
                "types-3.4/index.d.ts"
            ]
        }
    },
    "scripts": {
        "clean": "rimraf lib* types dist* *.tsbuildinfo",
        "compile-es6": "tsc --module es6 --outDir ./lib-es6",
        "compile-es6-watch": "npm run compile-es6 -- -w",
        "compile-umd": "tsc --module umd --outDir ./lib-umd",
        "compile-umd-watch": "npm run compile-umd -- -w",
        "bundle": "rollup -c",
        "bundle-watch": "npm run bundle -- -w",
        "minimize": "terser dist/index.js -c -m --source-map \"content='dist/index.js.map',url='index.min.js.map'\" -o dist/index.min.js",
        "build": "run-s gen-sample compile-umd",
        "watch": "npm-run-all compile-es6 -p compile-es6-watch bundle-watch",
        "gen-sample": "node ./util/genSample",
        "gen-sample-watch": "chokidar \"./samples/*.omd\" -c \"npm run gen-sample\"",
        "update": "npx npm-check-updates -u -t minor"
    },
    "dependencies": {
        "@hpcc-js/chart": "^2.72.0",
        "@hpcc-js/codemirror": "^2.55.0",
        "@hpcc-js/common": "^2.62.0",
        "@hpcc-js/comms": "^2.66.0",
        "@hpcc-js/ddl-shim": "^2.17.21",
        "@hpcc-js/dgrid": "^2.25.0",
        "@hpcc-js/eclwatch": "^2.67.0",
        "@hpcc-js/form": "^2.8.66",
        "@hpcc-js/graph": "^2.75.0",
        "@hpcc-js/layout": "^2.42.0",
        "@hpcc-js/map": "^2.70.0",
        "@hpcc-js/marshaller": "^2.23.103",
        "@hpcc-js/observable-md": "^2.48.0",
        "@hpcc-js/other": "^2.13.75",
        "@hpcc-js/phosphor": "^2.14.56",
        "@hpcc-js/util": "^2.42.0"
    },
    "devDependencies": {
        "@hpcc-js/bundle": "^2.10.21",
        "chokidar-cli": "^2.1.0",
        "d3-require": "^1",
        "npm-run-all": "4.1.5",
        "rimraf": "2.7.1",
        "rollup": "2.67.2",
        "@rollup/plugin-alias": "3.1.9",
        "@rollup/plugin-commonjs": "12.0.0",
        "@rollup/plugin-node-resolve": "8.4.0",
        "rollup-plugin-postcss": "3.1.8",
        "rollup-plugin-sourcemaps": "0.6.3",
        "terser": "4.8.0",
        "tslib": "2.3.1",
        "typescript": "4.5.5"
    }
}