{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "index.html",
            "type": "chrome",
            "request": "launch",
            "url": "file:///${workspaceRoot}/index.html",
            "runtimeArgs": [
                "--disable-web-security"
            ],
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "tmp.html",
            "type": "chrome",
            "request": "launch",
            "url": "file:///${workspaceRoot}/tmp.html",
            "runtimeArgs": [
                "--disable-web-security"
            ],
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "issue.html",
            "type": "chrome",
            "request": "launch",
            "url": "file:///${workspaceRoot}/issue.html",
            "runtimeArgs": [
                "--disable-web-security"
            ],
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "genSample",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/util/genSample.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}