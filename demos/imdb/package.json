{
    "private": true,
    "name": "@hpcc-js/demo-imdb",
    "scripts": {
        "clean": "rimraf --glob lib* types dist *.tsbuildinfo",
        "compile-umd": "tsc --module umd --outDir ./lib-umd",
        "compile-umd-watch": "npm run compile-umd -- -w",
        "build-docs": "typedoc --readme none --out ./doc ./src/index.ts",
        "build": "npm run compile-umd && npm run build-docs",
        "update": "npx --yes npm-check-updates -u -t minor"
    },
    "dependencies": {
        "@hpcc-js/common": "^2.73.0",
        "@hpcc-js/dgrid": "^2.34.0",
        "@hpcc-js/dgrid-shim": "^2.26.0",
        "@hpcc-js/graph": "^2.87.0",
        "@hpcc-js/layout": "^2.51.0",
        "@hpcc-js/phosphor": "^2.20.0",
        "@hpcc-js/util": "^2.53.0",
        "d3-fetch": "^1",
        "es6-promise": "4.2.8",
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "rimraf": "5.0.10"
    }
}