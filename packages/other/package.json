{
  "name": "@hpcc-js/other",
  "version": "0.0.51",
  "description": "hpcc-js - Viz Other",
  "main": "build/other.js",
  "module": "lib/index",
  "unpkg": "build/other.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "build/*",
    "lib/*",
    "src/*.css"
  ],
  "scripts": {
    "clean": "rimraf lib* && rimraf build",
    "compile": "tsc --module esnext --outDir ./lib",
    "compile-watch": "npm run compile -- -w",
    "compile-umd": "tsc",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs build/other.js -c -m --source-map  -o build/other.min.js",
    "build": "npm run compile && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-watch\" \"npm run bundle-watch\"",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/api": "^0.0.51",
    "@hpcc-js/common": "^0.0.51",
    "@hpcc-js/layout": "^0.0.51",
    "d3-array": "1.2.1",
    "d3-cloud": "1.2.4",
    "d3-collection": "1.0.4",
    "d3-format": "1.2.0",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-shape": "1.2.0",
    "d3-svg-legend": "2.24.1",
    "d3-time": "1.0.6",
    "d3-time-format": "2.1.0",
    "d3-timer": "1.0.6",
    "d3-zoom": "1.3.0",
    "javascript-autocomplete": "1.0.3",
    "simpleheat": "0.4.0",
    "tslib": "1.8.0"
  },
  "devDependencies": {
    "concurrently": "3.5.0",
    "rimraf": "2.6.1",
    "rollup": "0.51.7",
    "rollup-plugin-postcss": "0.5.5",
    "tslint": "5.8.0",
    "typedoc": "0.7.1",
    "typescript": "2.6.1",
    "uglify-es": "3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}
