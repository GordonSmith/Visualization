{
    "name": "@hpcc-js/dgrid-shim",
    "version": "2.22.1",
    "description": "dgrid shim",
    "main": "dist/index.js",
    "unpkg": "dist/index.min.js",
    "jsdelivr": "dist/index.min.js",
    "types": "types/index.d.ts",
    "typesVersions": {
        "<3.8": {
            "*": [
                "types-3.4/index.d.ts"
            ]
        }
    },
    "scripts": {
        "clean": "rimraf lib* types dist *.tsbuildinfo",
        "compile-es6": "tsc --module es6 --outDir ./lib-es6",
        "compile-es6-watch": "npm run compile-es6 -- -w",
        "compile-amd": "tsc --module amd --outDir ./lib-amd",
        "compile-cjs": "tsc --module commonjs --outDir ./lib-cjs",
        "compile-cjs-watch": "npm run compile-cjs -- -w",
        "bundle": "webpack",
        "bundle-watch": "npm run bundle -- -w",
        "minimize": "terser dist/index.js -c -m -o dist/index.min.js",
        "gen-legacy-types": "downlevel-dts ./types ./types-3.4",
        "build": "npm run compile-cjs && npm run bundle",
        "watch": "run-p compile-cjs-watch bundle-watch",
        "stamp": "node node_modules/@hpcc-js/bundle/src/stamp.js",
        "lint": "eslint src/**/*.ts",
        "update": "npx npm-check-updates -u -t minor"
    },
    "files": [
        "dist/**/*",
        "types/*"
    ],
    "devDependencies": {
        "@hpcc-js/bundle": "^2.10.23",
        "@types/dojo": "1.9.43",
        "@typescript-eslint/eslint-plugin": "4.31.0",
        "@typescript-eslint/parser": "4.31.0",
        "copy-webpack-plugin": "5.1.1",
        "css-loader": "3.4.1",
        "dgrid": "1.3.3",
        "dijit": "1.17.0",
        "dijit-themes": "1.17.0",
        "dojo": "1.17.0",
        "dojo-dstore": "1.2.1",
        "dojo-util": "1.17.0",
        "dojo-webpack-plugin": "2.8.20",
        "dojox": "1.17.0",
        "downlevel-dts": "0.6.0",
        "eslint": "7.32.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "npm-run-all": "4.1.5",
        "rimraf": "2.6.3",
        "style-loader": "1.1.2",
        "terser-webpack-plugin": "3.0.2",
        "url-loader": "3.0.0",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10",
        "webpack-hasjs-plugin": "1.0.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hpcc-systems/Visualization.git"
    },
    "keywords": [
        "dojo",
        "webpack",
        "dgrid",
        "dstore"
    ],
    "author": "Gordon Smith",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/hpcc-systems/Visualization/issues"
    },
    "homepage": "https://github.com/hpcc-systems/Visualization#readme"
}