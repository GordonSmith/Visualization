{
  "name": "@hpcc-js/loader",
  "version": "0.0.27",
  "description": "hpcc-js - Loader",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "lib-es6/*",
    "dist/*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "build-es6": "tsc --module es6 --outDir ./lib-es6",
    "bundle": "rollup -c",
    "clean": "rimraf lib* && rimraf dist*",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/requirejs-shim": "^0.0.26",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-watch": "^4.0.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
