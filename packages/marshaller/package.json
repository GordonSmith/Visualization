{
  "name": "@hpcc-js/marshaller",
  "version": "0.0.50",
  "description": "hpcc-js - Viz Marshaller",
  "main": "build/marshaller.js",
  "module": "lib/index",
  "unpkg": "build/marshaller.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "build/*",
    "lib/*",
    "src/*.css"
  ],
  "scripts": {
    "clean": "rimraf lib* && rimraf build",
    "compile": "tsc --module esnext --outDir ./lib",
    "compile-watch": "npm run compile -- -w",
    "compile-umd": "tsc",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs build/marshaller.js -c -m -o build/marshaller.min.js",
    "build": "npm run compile && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-watch\" \"npm run bundle-watch\"",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/chart": "^0.0.46",
    "@hpcc-js/common": "^0.0.44",
    "@hpcc-js/comms": "^0.0.45",
    "@hpcc-js/composite": "^0.0.47",
    "@hpcc-js/ddl-shim": "^0.0.15",
    "@hpcc-js/dgrid": "^0.0.48",
    "@hpcc-js/form": "^0.0.46",
    "@hpcc-js/graph": "^0.0.45",
    "@hpcc-js/layout": "^0.0.44",
    "@hpcc-js/other": "^0.0.44",
    "@hpcc-js/phosphor": "^0.0.46",
    "@hpcc-js/util": "^0.0.42",
    "d3-array": "1.2.1",
    "d3-collection": "1.0.4",
    "tslib": "1.8.0"
  },
  "devDependencies": {
    "@hpcc-js/bundle": "^0.0.15",
    "concurrently": "3.5.0",
    "rimraf": "2.6.1",
    "rollup": "0.51.3",
    "rollup-plugin-postcss": "0.5.5",
    "typedoc": "0.7.1",
    "typescript": "2.6.1",
    "uglify-js": "3.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}