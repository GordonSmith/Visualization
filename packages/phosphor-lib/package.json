{
  "name": "@hpcc-js/phosphor-lib",
  "version": "0.0.13",
  "description": "hpcc-js - Viz Phosphor Library",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
    "build": "npm run build-umd && npm run build-es6",
    "build-umd": "rollup -c -f umd -o lib/index.js",
    "build-es6": "rollup -c -f es -o lib-es6/index.js",
    "clean": "rimraf lib* && rimraf dist && rimraf build"
  },
  "dependencies": {
    "@phosphor/algorithm": "^1.1.0",
    "@phosphor/messaging": "^1.2.0",
    "@phosphor/widgets": "^1.2.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "^2.3.4",
    "cross-env": "^5.0.1",
    "rollup": "^0.43.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
