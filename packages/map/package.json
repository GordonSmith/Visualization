{
  "name": "@hpcc-js/map",
  "version": "0.0.49",
  "description": "hpcc-js - Viz Map",
  "main": "build/map.js",
  "module": "lib/index",
  "unpkg": "build/map.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "build/*",
    "lib/*",
    "TopoJSON/*",
    "src/*.css"
  ],
  "scripts": {
    "clean": "rimraf lib* && rimraf build",
    "compile": "tsc --module esnext --outDir ./lib",
    "compile-watch": "npm run compile -- -w",
    "compile-umd": "tsc",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs build/map.js -c -m -o build/map.min.js",
    "build": "npm run compile && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-watch\" \"npm run bundle-watch\"",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/api": "^0.0.45",
    "@hpcc-js/common": "^0.0.45",
    "@hpcc-js/graph": "^0.0.46",
    "@hpcc-js/layout": "^0.0.45",
    "@hpcc-js/other": "^0.0.45",
    "d3-collection": "1.0.4",
    "d3-format": "1.2.0",
    "d3-geo": "1.6.4",
    "d3-request": "1.0.5",
    "d3-selection": "1.1.0",
    "d3-tile": "0.0.4",
    "tslib": "1.8.0"
  },
  "devDependencies": {
    "concurrently": "3.5.0",
    "google-maps": "3.2.1",
    "rimraf": "2.6.1",
    "rollup": "0.51.3",
    "rollup-plugin-postcss": "0.5.5",
    "topojson": "3.0.2",
    "tslint": "5.8.0",
    "typedoc": "0.7.1",
    "typescript": "2.6.1",
    "uglify-js": "3.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}