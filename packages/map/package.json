{
  "name": "@hpcc-js/map",
  "version": "0.3.26",
  "description": "hpcc-js - Viz Map",
  "main": "dist/index.js",
  "module": "dist/index.es6",
  "unpkg": "dist/index.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "types/*",
    "TopoJSON/*",
    "src/*.css"
  ],
  "scripts": {
    "clean": "rimraf lib* || rimraf types || rimraf dist",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs dist/index.js -c -m --source-map  -o dist/index.min.js",
    "build": "npm run compile-es6 && npm run bundle",
    "watch": "run-p compile-es6-watch bundle-watch",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/api": "^0.5.3",
    "@hpcc-js/common": "^0.9.3",
    "@hpcc-js/graph": "^0.2.20",
    "@hpcc-js/layout": "^0.3.19",
    "@hpcc-js/other": "^0.5.19",
    "d3-collection": "1.0.4",
    "d3-format": "1.3.0",
    "d3-geo": "1.6.4",
    "d3-quadtree": "1.0.3",
    "d3-request": "1.0.5",
    "d3-selection": "1.1.0",
    "d3-tile": "0.0.4",
    "google-maps": "3.2.1",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@hpcc-js/bundle": "^0.0.45",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.63.5",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-postcss": "1.6.2",
    "topojson": "3.0.2",
    "tslint": "5.9.1",
    "typedoc": "0.7.1",
    "typescript": "3.0.1",
    "uglify-es": "3.3.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}