{
    "name": "@hpcc-js/leaflet-shim",
    "version": "2.1.15",
    "description": "hpcc-js - leaflet shim",
    "main": "dist/index.js",
    "unpkg": "dist/index.min.js",
    "jsdelivr": "dist/index.min.js",
    "types": "types/index.d.ts",
    "typesVersions": {
        "<3.8": {
            "*": [
                "types-3.4/index.d.ts"
            ]
        }
    },
    "files": [
        "dist/*",
        "types/*",
        "types-3.4/*",
        "src/*"
    ],
    "scripts": {
        "clean": "rimraf lib* types dist *.tsbuildinfo",
        "compile-es6": "tsc --module es6 --outDir ./lib-es6",
        "compile-es6-watch": "npm run compile-es6 -- -w",
        "compile-umd": "tsc --module umd --outDir ./lib-umd",
        "compile-umd-watch": "npm run compile-umd -- -w",
        "bundle": "webpack",
        "bundle-watch": "npm run bundle -- -w",
        "gen-legacy-types": "downlevel-dts ./types ./types-3.4",
        "build": "npm run compile-es6 && npm run bundle",
        "watch": "npm-run-all compile-es6 -p compile-es6-watch bundle-watch",
        "lint": "eslint src/**/*.ts"
    },
    "dependencies": {
        "@types/leaflet": "1.5.1",
        "leaflet": "1.5.1",
        "leaflet.gridlayer.googlemutant": "https://github.com/GordonSmith/Leaflet.GridLayer.GoogleMutant#master"
    },
    "devDependencies": {
        "@types/leaflet-draw": "1.0.0",
        "@types/leaflet.heat": "0.2.0",
        "@types/leaflet.markercluster": "1.4.0",
        "@typescript-eslint/eslint-plugin": "4.31.0",
        "@typescript-eslint/parser": "4.31.0",
        "css-loader": "3.4.1",
        "d3-geo": "^1",
        "d3-selection": "^1",
        "eslint": "7.32.0",
        "leaflet-draw": "1.0.4",
        "leaflet.heat": "0.2.0",
        "leaflet.markercluster": "1.4.1",
        "npm-run-all": "4.1.5",
        "rimraf": "2.6.3",
        "simpleheat": "0.4.0",
        "style-loader": "1.1.2",
        "terser-webpack-plugin": "3.0.2",
        "tslib": "2.3.0",
        "typescript": "4.3.4",
        "url-loader": "3.0.0",
        "webpack": "4.41.5",
        "webpack-cli": "3.3.10"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hpcc-systems/Visualization.git"
    },
    "author": "Gordon Smith <gordonjsmith@gmail.com>",
    "contributors": [],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/hpcc-systems/Visualization/issues"
    },
    "homepage": "https://github.com/hpcc-systems/Visualization",
    "gitHead": "90532e4914f063f90c74dbb392c074f783440017"
}