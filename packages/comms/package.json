{
  "name": "@hpcc-js/comms",
  "version": "0.0.44",
  "description": "hpcc-js - Communications",
  "main": "lib/index-node.js",
  "module": "lib-es6/index-node.js",
  "browser": "lib-es6/index.js",
  "types": "lib/index-node.d.ts",
  "unpkg": "dist/comms.min.js",
  "files": [
    "lib/*",
    "lib-es6/*",
    "src/*.css",
    "dist/*"
  ],
  "scripts": {
    "clean": "rimraf lib* && rimraf dist*",
    "build": "tsc",
    "build-es6": "tsc --module esnext --outDir ./lib-es6",
    "watch-es6": "tsc -w --module esnext --outDir ./lib-es6",
    "bundle": "node ./node_modules/@hpcc-js/bundle/lib/rollup",
    "watch-bundle": "node ./node_modules/@hpcc-js/bundle/lib/watch",
    "watch": "concurrently --kill-others \"npm run watch-es6\" \"npm run watch-bundle\"",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/util": "^0.0.41",
    "d3-request": "1.0.5",
    "d3-time-format": "2.0.5",
    "es6-promise": "4.1.1",
    "request": "2.81.0",
    "safe-buffer": "5.0.1",
    "tmp": "0.0.33",
    "tslib": "1.8.0",
    "xmldom": "0.1.27"
  },
  "devDependencies": {
    "@hpcc-js/bundle": "^0.0.14",
    "@types/d3-request": "1.0.2",
    "@types/request": "0.0.41",
    "@types/xmldom": "0.1.29",
    "concurrently": "3.5.0",
    "rimraf": "2.6.1",
    "typedoc": "0.7.1",
    "typescript": "2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}