{
  "name": "@hpcc-js/eclwatch",
  "version": "2.7.1",
  "description": "'ECL Watch' Web interface for HPCC Platform.",
  "main": "dist/index.js",
  "module": "dist/index.es6",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rimraf lib* types dist *.tsbuildinfo",
    "copy-res-es6-promise": "cpx \"../../node_modules/es6-promise/dist/es6-promise.auto.min.js\" ./dist/dist",
    "copy-res-eclwatch-img": "cpx \"../../eclwatch/img/**/*.{png,jpg,gif}\" ./dist/eclwatch/img/",
    "copy-res-eclwatch-ecl": "cpx \"../../eclwatch/ecl/**/*.*\" ./dist/eclwatch/ecl/",
    "copy-res-dojo": "cpx \"../../node_modules/dojo/resources/**/*.{png,jpg,gif}\" ./dist/node_modules/dojo/resources/",
    "copy-res-dojox": "cpx \"../../node_modules/dojox/widget/ColorPicker/images/**/*.{png,jpg,gif}\" ./dist/eclwatch/img/",
    "copy-res-TopoJSON": "cpx \"../../node_modules/@hpcc-js/map/TopoJSON/**/*\" ./dist/dist/TopoJSON/",
    "copy-res-font-awesome": "cpx \"../../node_modules/font-awesome/**/*\" ./dist/dist/font-awesome/",
    "copy-res-stub_htm": "cpx \"./stub.htm\" ./dist/",
    "copy-res": "run-p copy-res-es6-promise copy-res-eclwatch-img copy-res-eclwatch-ecl copy-res-dojo copy-res-dojox copy-res-TopoJSON copy-res-font-awesome copy-res-stub_htm",
    "compile-amd": "tsc",
    "compile-watch": "npm run compile-amd -- -w",
    "bundle": "webpack --env production --config webpack.config.js",
    "bundle-dev": "webpack --env development --config webpack.config.js",
    "bundle-watch": "npm run bundle-dev -- --watch",
    "build": "npm-run-all --parallel copy-res compile-amd --serial bundle",
    "build-dev": "npm-run-all --parallel copy-res compile-amd --serial bundle-dev",
    "dev-start-ws": "ws",
    "dev-start": "run-p bundle-watch dev-start-ws",
    "dev-start-verbose": "ws --verbose.include request response",
    "jslint": "jshint --config ./.jshintrc ./eclwatch",
    "tslint": "tslint --project . src/**/*.ts",
    "lint": "run-s jslint tslint"
  },
  "dependencies": {},
  "devDependencies": {
    "@hpcc-js/chart": "^2.26.2",
    "@hpcc-js/common": "^2.21.1",
    "@hpcc-js/codemirror": "^2.12.2",
    "@hpcc-js/comms": "^2.12.0",
    "@hpcc-js/dgrid": "^2.7.3",
    "@hpcc-js/graph": "^2.13.1",
    "@hpcc-js/html": "^2.8.2",
    "@hpcc-js/layout": "^2.16.7",
    "@hpcc-js/map": "^2.14.5",
    "@hpcc-js/other": "^2.13.10",
    "@hpcc-js/phosphor": "^2.13.10",
    "@hpcc-js/timeline": "^2.7.10",
    "@hpcc-js/tree": "^2.9.1",
    "@hpcc-js/util": "^2.10.0",
    "clipboard": "2.0.4",
    "codemirror": "5.50.2",
    "dijit": "1.16.0",
    "dojo": "1.16.0",
    "dojo-themes": "1.14.1",
    "dojo-util": "1.16.0",
    "dojox": "1.16.0",
    "es6-promise": "4.2.8",
    "font-awesome": "4.7.0",
    "@types/dojo": "1.9.42",
    "copy-webpack-plugin": "5.1.1",
    "cpx": "1.5.0",
    "css-loader": "3.4.1",
    "dojo-webpack-plugin": "2.8.12",
    "file-loader": "5.0.2",
    "jshint": "2.10.2",
    "local-web-server": "3.0.7",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "style-loader": "1.1.2",
    "tslib": "1.10.0",
    "tslint": "5.17.0",
    "typescript": "3.6.4",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
