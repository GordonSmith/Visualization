{
  "name": "@hpcc-js/common",
  "version": "0.0.68",
  "description": "hpcc-js - Viz Common",
  "main": "build/index.js",
  "module": "build/index.es6",
  "unpkg": "build/index.min.js",
  "types": "types/index.d.ts",
  "files": [
    "build/*",
    "types/*",
    "src/*.css",
    "font-awesome/**/*"
  ],
  "scripts": {
    "clean": "rimraf lib* && rimraf types && rimraf build",
    "copy-resources": "cpx \"../../node_modules/font-awesome/fonts/*\" \"./font-awesome/fonts\" && cpx \"../../node_modules/font-awesome/css/*\" \"./font-awesome/css\"",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs build/index.js -c -m --source-map  -o build/index.min.js",
    "build": "npm run compile-es6 && npm run copy-resources && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-watch\" \"npm run bundle-watch\"",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/util": "^0.0.66"
  },
  "devDependencies": {
    "colorbrewer": "1.0.0",
    "concurrently": "3.5.0",
    "cpx": "1.5.0",
    "d3-array": "1.2.1",
    "d3-brush": "1.0.4",
    "d3-collection": "1.0.4",
    "d3-dispatch": "1.0.3",
    "d3-drag": "1.2.1",
    "d3-dsv": "1.0.5",
    "d3-format": "1.2.0",
    "d3-interpolate": "1.1.5",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-time-format": "2.1.0",
    "d3-transition": "1.1.0",
    "d3-zoom": "1.3.0",
    "es6-promise": "4.1.1",
    "font-awesome": "4.7.0",
    "rimraf": "2.6.2",
    "rollup": "0.54.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-postcss": "0.5.5",
    "tslib": "1.8.0",
    "tslint": "5.8.0",
    "typedoc": "0.7.1",
    "typescript": "2.6.1",
    "uglify-es": "3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}
