{
  "private": true,
  "name": "@hpcc-js/test-common",
  "version": "0.0.53",
  "dependencies": {
    "@hpcc-js/common": "^0.0.69",
    "chai": "4.0.0",
    "es6-promise": "4.1.1",
    "mocha": "3.4.2",
    "simulate-event": "1.4.0"
  },
  "devDependencies": {
    "@types/chai": "3.5.2",
    "@types/mocha": "2.2.41",
    "concurrently": "3.5.0",
    "css-loader": "0.26.1",
    "file-loader": "1.1.5",
    "mocha-chrome": "1.0.3",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "typescript": "2.6.1",
    "webpack": "3.10.0"
  },
  "scripts": {
    "clean": "rimraf lib* && rimraf types && rimraf dist",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "bundle": "webpack",
    "bundle-watch": "npm run bundle -- --watch",
    "build": "npm run compile-es6 && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-es6-watch\" \"npm run bundle-watch\"",
    "test": "mocha-chrome ./test.html"
  }
}