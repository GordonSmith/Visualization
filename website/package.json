        "compile-umd": "run-p compile-util compile-web",
        "generate": "node ./lib-umd/index.js",
        "build": "run-s compile-umd generate",
        "watch": "run-p compile-util-watch compile-web-watch",
        "test": "echo \"Error: no test specified\" && exit 1"
