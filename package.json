  "scripts": {
    "test": "gulp lint && gulp jscs && gulp unitTest && gulp build-all && gulp unitTestBuild && gulp unitTestBuildNonAMD",
    "watch": "tsc -w",
    "rollup": "rollup --config rollup.config.js",
    "compile-es6": "tsc --project tsc-compile-es6.json",
    "link-amd": "rollup --format amd --name hpcc-viz-common obj-es6/common.js --output dist-amd/hpcc-viz-common.js",
