    "link-umd": "rollup --format umd --name hpcc-viz-common obj-es6/common.js --output dist-umd/hpcc-viz-common.js",
    "link-es6": "rollup obj-es6/common.js --output dist-es6/hpcc-viz-common.js"
  },
